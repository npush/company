<?xml version="1.0"?>
<config>
    <modules>
        <Stableflow_Uploader>
            <version>1.1.1</version>
        </Stableflow_Uploader>
    </modules>
    <global>
        <blocks>
            <adminhtml>
                <rewrite>
                    <media_uploader>Stableflow_Uploader_Block_Media_Uploader</media_uploader>
                </rewrite>
            </adminhtml>
        </blocks>
    </global>
    <adminhtml>
        <layout>
            <updates>
                <dull_uploader module="Stableflow_Uploader">
                    <file>js_uploader.xml</file>
                </dull_uploader>
            </updates>
        </layout>
    </adminhtml>
</config>