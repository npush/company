<?xml version="1.0"?>
<config>
    <admin>
        <routers>
            <catsidebarnav>
                <use>admin</use>
                <args>
                    <module>Magebuzz_Catsidebarnav</module>
                    <frontName>catsidebarnav</frontName>
                </args>
            </catsidebarnav>
        </routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <catsidebarnav>
                    <file>catsidebarnav.xml</file>
                </catsidebarnav>
            </updates>
        </layout>
        <translate>
            <modules>
                <Magebuzz_Catsidebarnav>
                    <files>
                        <default>Magebuzz_Catsidebarnav.csv</default>
                    </files>
                </Magebuzz_Catsidebarnav>
            </modules>
        </translate>
    </adminhtml>
    <acl>
        <resources>
            <all>
                <title>Allow Everything</title>
            </all>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <catsidebarnav translate="title">
                                        <title>Catsidebarnav Permission Setting</title>
                                        <sort_order>50</sort_order>
                                    </catsidebarnav>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
</config>