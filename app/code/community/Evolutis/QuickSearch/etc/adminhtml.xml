	<acl>
		<resources>
			<admin>
				<children>
					<system>
						<children>
							<config>
								<children>
									<!-- Champs permission pour le module dans l'interface System -->
									<evolutis_quicksearch module="quicksearch">
										<title>Evolutis</title>
										<sort_order>9999</sort_order>
									</evolutis_quicksearch>
								</children>
							</config>
						</children>
					</system>
				</children>
			</admin>
		</resources>
	</acl>
</config>