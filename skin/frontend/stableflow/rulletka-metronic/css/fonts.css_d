html {
    text-rendering: optimizeLegibility !important;
    -webkit-font-smoothing: antialiased;
}
body {
    font-size: 100%;
    padding: 0;
    margin: 0;
    font-family: 'OpenSansRegular';
}
ul,
ol {
    margin: 0;
}
ul ul,
ol ul,
ul ol,
ol ol {
    margin-top: 0;
}
p {
    font-size: 1rem;
    line-height: 1.5;
}
li,
dt,
dd {
    font-size: 1rem;
    line-height: 1.5;
    margin: 1.5rem 0 0;
}
dd {
    margin-top: 0;
    margin-left: 0;
}
li {
    margin: 0;
}
blockquote {
    font-style: italic;
    margin: 0 1.5rem;
}
pre {
    display: block;
}
pre code {
    white-space: pre-wrap;
    padding: 0;
}
code {
    font-size: 16px;
}